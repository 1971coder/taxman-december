{
  "name": "taxman-december",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.8",
  "scripts": {
    "dev": "pnpm --parallel --filter ./app/* run dev",
    "dev:backend": "pnpm --filter @taxman/backend run dev",
    "dev:frontend": "pnpm --filter @taxman/frontend run dev",
    "lint": "pnpm --recursive run lint",
    "test": "pnpm --recursive run test",
    "typecheck": "pnpm --recursive run typecheck",
    "format": "pnpm prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "db:push": "pnpm --filter @taxman/backend run db:push"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  }
}
